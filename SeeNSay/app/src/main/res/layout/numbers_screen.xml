        android:layout_height="120dp"
        android:padding="10dp" />

    <ImageView
        android:id="@+id/number11"
        android:layout_above="@+id/number10"
        android:layout_toRightOf="@+id/button1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="10dp"
        android:src="@mipmap/eleven" />
    <ImageView
        android:id="@+id/number11pane"
        android:layout_width="120dp"
        android:layout_toRightOf="@id/button1"
        android:layout_above="@+id/number10"
        android:layout_height="120dp"
        android:padding="10dp" />

    <ImageView
        android:id="@+id/number12"
        android:layout_above="@+id/number11"
        android:layout_toRightOf="@+id/button1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="10dp"
        android:src="@mipmap/twelve" />
    <ImageView
        android:id="@+id/number12pane"
        android:layout_width="120dp"
        android:layout_toRightOf="@id/button1"
        android:layout_above="@+id/number11"
        android:layout_height="120dp"
        android:padding="10dp" />

    <Button
        android:id="@+id/button1"
        android:layout_width="100dp"
        android:layout_height="490dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />



</RelativeLayout>



