        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="10dp"
        android:src="@mipmap/three" />
    <ImageView
        android:id="@+id/number3pane"
        android:layout_width="120dp"
        android:layout_toLeftOf="@id/button1"
        android:layout_above="@+id/number4"
        android:layout_height="120dp"
        android:padding="10dp" />

    <ImageView
        android:id="@+id/number4"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/button1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="10dp"
        android:src="@mipmap/four" />
    <ImageView
        android:id="@+id/number4pane"
        android:layout_width="120dp"
        android:layout_toLeftOf="@id/button1"
        android:layout_centerVertical="true"
        android:layout_height="120dp"
        android:padding="10dp" />

    <ImageView
        android:id="@+id/number5"
        android:layout_below="@+id/number4"
        android:layout_toLeftOf="@id/button1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:padding="10dp"
        android:src="@mipmap/five" />
    <ImageView
        android:id="@+id/number5pane"
        android:layout_width="120dp"
        android:layout_toLeftOf="@id/button1"
